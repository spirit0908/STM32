<?xml version="1.0"?>
<span>
  <generatorConfig>
    <ConfigName>ConfigTest</ConfigName>
    <outputPath>./output/generate/</outputPath>
    <mcUid>000</mcUid>
    <mcAddress>0x48</mcAddress>
  </generatorConfig> 
  <Interfaces>
    <CAN>
      <enable>true</enable>
    </CAN>
    <SERIAL>
      <enable>true</enable>
    </SERIAL>
  </Interfaces>
  <switch>
    <pushButton id="1">
      <name>PB_UP</name>
      <port>GPIOA</port>
      <pin>PA0</pin>
      <pushConfig>
        <shortPush>
          <action>LcdMenu_MenuUp</action>
          <address></address>
        </shortPush>
        <longPush>
          <action>LcdMenu_MenuTop</action>
          <address></address>
        </longPush>
        <configPush>
          <action>NULL</action>
          <address></address>
        </configPush>
      </pushConfig>
      <ledStatus>
        <activated>no</activated>
        <pushConfigId>0</pushConfigId>
        <fondu>yes</fondu>
      </ledStatus>
    </pushButton>
    <pushButton id="2">
      <name>PB_DOWN</name>
      <port>GPIOC</port>
      <pin>PC14</pin>
      <pushConfig>
        <shortPush>
          <action>LcdMenu_MenuDown</action>
          <address></address>
        </shortPush>
        <longPush>
          <action>NULL</action>
          <address></address>
        </longPush>
        <configPush>
          <action>NULL</action>
          <address></address>
        </configPush>
      </pushConfig>
      <ledStatus>
        <activated>no</activated>
        <pushConfigId>0</pushConfigId>
        <fondu>yes</fondu>
      </ledStatus>
    </pushButton>
    <pushButton id="3">
      <name>PB_LEFT</name>
      <port>GPIOC</port>
      <pin>PC15</pin>
      <pushConfig>
        <shortPush>
          <action>LcdMenu_MenuDown</action>
          <address></address>
        </shortPush>
        <longPush>
          <action>NULL</action>
          <address></address>
        </longPush>
        <configPush>
          <action>NULL</action>
          <address></address>
        </configPush>
      </pushConfig>
      <ledStatus>
        <activated>no</activated>
        <pushConfigId>0</pushConfigId>
        <fondu>yes</fondu>
      </ledStatus>
    </pushButton>
    <pushButton id="4">
      <name>PB_RIGHT</name>
      <port>GPIOA</port>
      <pin>PA1</pin>
      <pushConfig>
        <shortPush>
          <action>LcdMenu_MenuDown</action>
          <address></address>
        </shortPush>
        <longPush>
          <action>NULL</action>
          <address></address>
        </longPush>
        <configPush>
          <action>NULL</action>
          <address></address>
        </configPush>
      </pushConfig>
      <ledStatus>
        <activated>no</activated>
        <pushConfigId>0</pushConfigId>
        <fondu>yes</fondu>
      </ledStatus>
    </pushButton>
  </switch>
  <lights>
    <light id="1">
      <name>RGB_R</name>
      <type>
        <active>yes</active>
        <dimmer>no</dimmer>
        <timer>no</timer>
        <ledStrip>yes</ledStrip>
      </type>
      <port>GPIOB</port>
      <pin>PB0</pin>
      <defaultBrightness></defaultBrightness>
      <defaultTimer></defaultTimer>
    </light>
    <light id="2">
      <name>RGB_G</name>
      <type>
        <active>yes</active>
        <dimmer>no</dimmer>
        <timer>no</timer>
        <ledStrip>yes</ledStrip>
      </type>
      <port>GPIOA</port>
      <pin>PA7</pin>
      <defaultBrightness></defaultBrightness>
      <defaultTimer></defaultTimer>
    </light>
    <light id="3">
      <name>RGB_B</name>
      <type>
        <active>yes</active>
        <dimmer>no</dimmer>
        <timer>no</timer>
        <ledStrip>yes</ledStrip>
      </type>
      <port>GPIOA</port>
      <pin>PA6</pin>
      <defaultBrightness></defaultBrightness>
      <defaultTimer></defaultTimer>
    </light>
  </lights>
  <heating>
    <heater id="1">
      <name>Heater1</name>
      <zone>Zone1</zone>
      <type>elec</type> <!-- elec/chaudiere -->
      <port>GPIOA</port>
      <pin>PA4</pin>
      <temp_default>19.0</temp_default>
      <temp_confort>21.0</temp_confort>
      <mode_gestion>MODE4</mode_gestion>
    </heater>
    <heater id="2">
      <name>Heater2</name>
      <zone>Zone2</zone>
      <type>elec</type> <!-- elec/chaudiere -->
      <port>GPIOA</port>
      <pin>PA5</pin>
      <temp_default>18.0</temp_default>
      <temp_confort>22.0</temp_confort>
      <mode_gestion>MODE4</mode_gestion>
    </heater>
  </heating>
</span>

