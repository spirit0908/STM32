        <shortPush>
          <action>NULL</action>
          <address></address>
          <deviceType></deviceType>
          <action></action>
        </shortPush>
        <longPush>
          <action>NULL</action>
          <address></address>
          <deviceType></deviceType>
          <action></action>
        </longPush>
        <configPush>
          <action>NULL</action>
          <address></address>
          <deviceType></deviceType>
          <action></action>
        </configPush>
      </pushConfig>
      <ledStatus>
        <activated>no</activated>
        <pushConfigId>0</pushConfigId>
        <fondu>yes</fondu>
      </ledStatus>
    </pushButton>
  </switch>
  <lights>
    <light id="1">
      <name>RELAY_1</name>
      <type>
        <active>yes</active>
        <dimmer>no</dimmer>
        <timer>no</timer>
        <ledStrip>no</ledStrip>
      </type>
      <port>GPIOB</port>
      <pin>PB12</pin>
      <defaultBrightness></defaultBrightness>
      <defaultTimer></defaultTimer>
    </light>
    <light id="2">
      <name>RELAY_2</name>
      <type>
        <active>yes</active>
        <dimmer>no</dimmer>
        <timer>no</timer>
