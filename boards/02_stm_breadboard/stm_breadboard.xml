      <port>GPIOB</port>
      <pin>PB13</pin>
      <defaultBrightness></defaultBrightness>
      <defaultTimer></defaultTimer>
    </light>
    <light id="3">
      <name>RELAY_3</name>
      <type>
        <active>yes</active>
        <dimmer>no</dimmer>
        <timer>no</timer>
        <ledStrip>no</ledStrip>
      </type>
      <port>GPIOB</port>
      <pin>PB14</pin>
      <defaultBrightness></defaultBrightness>
      <defaultTimer></defaultTimer>
    </light>
    <light id="3">
      <name>RELAY_4</name>
      <type>
        <active>yes</active>
