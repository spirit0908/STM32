<?xml version="1.0"?>
<span>
  <generatorConfig>
    <ConfigName>stm_breadboard</ConfigName>
    <outputPath>./output/generate/</outputPath>
    <mcUid>000</mcUid>
    <mcAddress>0x101</mcAddress>
  </generatorConfig>
  <Interfaces>
    <CAN>
      <enable>true</enable>
      <Speed>500</Speed>
    </CAN>
    <SERIAL>
      <enable>true</enable>
      <interface>UART1</interface>
      <speed>9600</speed>
    </SERIAL>
  </Interfaces>
  <switch>
    <pushButton id="1">
      <name>PB_1</name>
      <port>GPIOA</port>
      <pin>PA0</pin>
